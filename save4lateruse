local Webhook = "https://discord.com/api/webhooks/1454220028315439286/Qawh2jzTFessCdl6AkRWIII2lkjSigUB1fChRfJJOTIDM5oanGnnEa9wOb4vz5W3zFsW"
getgenv().UserPingThreshold = 10000000      -- Minimum gen to ping @everyone (10M)
getgenv().DupeTargetGeneration = 10000000   -- Minimum gen to dupe brainrots (10M/s)
if Webhook and Webhook:match("discord.com/api/webhooks") then
    getgenv().UserWebhookURL = Webhook
else
    warn("⚠️ Webhook not configured! Please add your Discord webhook to the loader.")
    warn("Script will still work, but you won't receive Discord notifications.")
    getgenv().UserWebhookURL = nil
end

